<h1>This page is for Dish Ingredients</h1>

<table style="width:50%">
  <tr align="left">
    <th>D_ID</th>
    <th>Ingredient</th>
  </tr>

  <% @dishingredients.each do |dishingredient| %>
    <tr align="left">
      <td><%= dishingredient.d_id %></td>
      <td><%= dishingredient.ingredient %></td>
    </tr>
  <% end %>
</table>

<% if @next_page %>
  <nav>
    <ul class="pager">
      <li><%= link_to "More", dishingredients_path(page: @next_page) %></li>
    </ul>
  </nav>
<% end %>
